<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="DentalClinic">
	
	
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>consultorio.modelo.Cambio</class>
		<class>consultorio.modelo.Configuracion</class>
		<class>consultorio.modelo.Doctor</class>
		<class>consultorio.modelo.Funcionalidad</class>
		<class>consultorio.modelo.Login</class>
		<class>consultorio.modelo.Moneda</class>
		<class>consultorio.modelo.Paciente</class>
		<class>consultorio.modelo.Perfil</class>
		<class>consultorio.modelo.Perfil_Funcionalidad</class>
		<class>consultorio.modelo.Servicio</class>
		<class>consultorio.modelo.Usuario</class>
		<class>consultorio.modelo.Cita</class>
		<class>consultorio.modelo.Producto</class>
		<class>consultorio.modelo.Servcios_Productos</class>
	
		<properties>
			<property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect" />
			<property name="hibernate.hbm2ddl.auto" value="update" />
			<property name="hibernate.show_sql" value="update" />
			<property name="javax.persistence.jdbc.url"
				value="jdbc:postgresql://localhost:5432/consultorio" />
			<property name="javax.persistence.jdbc.user" value="postgres" />
			<property name="javax.persistence.jdbc.password" value="123" />
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver" />
		</properties>
	</persistence-unit>
	
</persistence>
